<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @photo.name %>
</p>

<p>
  <b>Description:</b>
  <%= @photo.description %>
</p>

<p>
<%= image_tag @photo.image.url(:large) %>
</p>

<p>
  <ul>
  <% @photo.tags.each do |tag| %>
    <li><%= tag.name %> </li>
  <% end %>
  </ul>
</p>

<br />
<%= link_to 'Edit', edit_photo_path(@photo) %> |
<%= link_to 'Back', photos_path %>
